{
    "name": "enoliu/thinkphp-oss",
    "description": "Thinkphp Flysystem for the AliYun OSS storage.",
    "license": "MIT",
    "authors": [
        {
            "name": "enoliu",
            "email": "595929049@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.0.0",
        "enoliu/flysystem-oss": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Enoliu\\Thinkphp\\Oss\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "Enoliu\\Thinkphp\\Oss\\OssService"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/jiangslee/flysystem-oss"
        },
        {
            "type": "git",
            "url": "https://github.com/jiangslee/thinkphp6-filesystem-oss-adapter"
        },
        {
            "type": "composer",
            "url": "https://packagist.org",
            "exclude": ["enoliu/flysystem-oss", "iidestiny/flysystem-oss"]
        }
    ]
}
